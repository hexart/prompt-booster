{
  "name": "prompt-booster",
  "version": "0.5.0",
  "description": "Prompt Booster Desktop App",
  "private": true,
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hexart/prompt-booster.git"
  },
  "scripts": {
    "dev": "npm run build:web && electron .",
    "build": "npm run build:web && electron-builder",
    "build:web": "cd ../web && pnpm build",
    "build:mac": "npm run build:web && electron-builder --mac --arm64",
    "build:mac:universal": "npm run build:web && electron-builder --mac --universal",
    "build:win": "npm run build:web && electron-builder --win",
    "build:win:x64": "npm run build:web && electron-builder --win --x64",
    "build:win:arm64": "npm run build:web && electron-builder --win --arm64",
    "build:linux": "npm run build:web && electron-builder --linux",
    "build:linux:x64": "npm run build:web && electron-builder --linux --x64",
    "build:linux:arm64": "npm run build:web && electron-builder --linux --arm64",
    "build:linux:deb:x64": "npm run build:web && electron-builder --linux deb --x64",
    "build:linux:rpm:x64": "npm run build:web && electron-builder --linux rpm --x64",
    "build:linux:appimage:x64": "npm run build:web && electron-builder --linux AppImage --x64",
    "build:linux:deb:arm64": "npm run build:web && electron-builder --linux deb --arm64",
    "build:linux:rpm:arm64": "npm run build:web && electron-builder --linux rpm --arm64",
    "build:linux:appimage:arm64": "npm run build:web && electron-builder --linux AppImage --arm64"
  },
  "keywords": [
    "Prompt Booster"
  ],
  "author": "Hexart Studio",
  "license": "MIT",
  "devDependencies": {
    "electron": "^36.2.0",
    "electron-builder": "^26.0.12",
    "node-gyp": "^11.2.0",
    "vite": "^6.3.5"
  }
}