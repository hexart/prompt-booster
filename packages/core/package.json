{
  "type": "module",
  "name": "@prompt-booster/core",
  "version": "0.8.0",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "compilerOptions": {
    "paths": {
      "@prompt-booster/api": [
        "../api/src"
      ],
      "@prompt-booster/api/*": [
        "../api/src/*"
      ]
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "eslint \"src/**/*.ts\"",
    "clean": "rm -rf .turbo node_modules dist"
  },
  "dependencies": {
    "@prompt-booster/api": "workspace:*",
    "uuid": "^13.0.0",
    "zustand": "^5.0.8"
  },
  "peerDependencies": {
    "i18next": "^25.2.1",
    "react": "^19.1.0",
    "react-i18next": "^15.5.3"
  },
  "devDependencies": {}
}